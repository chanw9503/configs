#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chanwoo-park/Desktop/project/eslint-configs/node_modules/.pnpm/nanobundle@2.1.0_typescript@5.7.2/node_modules/nanobundle/node_modules:/Users/chanwoo-park/Desktop/project/eslint-configs/node_modules/.pnpm/nanobundle@2.1.0_typescript@5.7.2/node_modules:/Users/chanwoo-park/Desktop/project/eslint-configs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chanwoo-park/Desktop/project/eslint-configs/node_modules/.pnpm/nanobundle@2.1.0_typescript@5.7.2/node_modules/nanobundle/node_modules:/Users/chanwoo-park/Desktop/project/eslint-configs/node_modules/.pnpm/nanobundle@2.1.0_typescript@5.7.2/node_modules:/Users/chanwoo-park/Desktop/project/eslint-configs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanobundle/bin.min.mjs" "$@"
else
  exec node  "$basedir/../nanobundle/bin.min.mjs" "$@"
fi
